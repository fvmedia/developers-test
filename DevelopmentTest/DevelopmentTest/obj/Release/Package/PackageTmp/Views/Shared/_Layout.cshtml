<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title -  Firstview UI Developers Test</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
   

    <div class="col-lg-12 col-md-12 col-xs-12 cssload-preloader">

        <div class="cssload-preloader-box">
            <div>				I			</div>
            <div>				N			</div>
            <div>				T			</div>
            <div>				E			</div>
            <div>				R			</div>
            <div>				V			</div>
            <div>				I			</div>
            <div>				E			</div>
            <div>				W			</div>
        </div>
        <div class="cssload-preloader-box" style=" margin-top: 60px;">
            <div>				T			</div>
            <div>				E			</div>
            <div>				S			</div>
            <div>				T			</div>
        </div>
    </div>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    @Html.ActionLink("Firstview UI Developers Test", "Create", "Form", new { area = "" }, new { @class = "navbar-brand" })
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        
        <footer>
            <p>&copy; @DateTime.Now.Year -  Firstview UI Developers Test</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <link rel="stylesheet" href="http://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
    <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script type="text/javascript">
        $(function () {

            $('.date-picker').datepicker();
        })
    </script>

    <script type="text/javascript">
        $(function () {
            setTimeout(function () {
                $(".cssload-preloader").fadeOut("slow");
            }, 1000)
        });
        $(function () { $('label').text("") }); 

    </script>

</body>
</html>
